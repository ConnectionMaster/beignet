	SHADER test_barrier

	VERSION_2_1

	DCL_THREADGROUP VARIABLE;

	DCL_UAVRAW u0, TRUE =
	{
	  KERNEL_ARGUMENT,0
	};

	DCL_UAVRAW u1, TRUE =
	{
	  KERNEL_ARGUMENT,4
	};

	DCL_CONSTANTREGISTER c0 =
	{
		KERNEL_ARGUMENT, 1, 0,
		KERNEL_ARGUMENT, 2, 0,
		KERNEL_ARGUMENT, 3, 0,
		KERNEL_ARGUMENT, 5, 0
	};

	DCL_CONSTANTREGISTER c1 =
	{
		KERNEL_ARGUMENT, 6, 0,
		KERNEL_ARGUMENT, 7, 0,
		UNUSED, 0, 0,
		UNUSED, 0, 0
	};

	DCL_INPUT i0.xyz, THREAD_ID;

	DCL_TEMPS r0..r23;
	DCL_POINTERS ptr0..ptr3;
	ADDRESS_OF ptr0, u0;
	MOV r0, c0.x;
	MOV r1, c0.y;
	MOV r2, c0.z;
	ADDRESS_OF ptr1, u1;
	MOV r3, c0.w;
	MOV r4, c1.x;
	MOV r5, c1.y;


	FTOI r12.x, r0.x;
	BTOI r13.x, r1.x;
	IADD r14.x, r12.x, r13.x;
	IADD r15.x, r14.x, r2.x;
	STOI r16.x, r3.x;
	IADD r17.x, r15.x, r16.x;
	IADD r18.x, r17.x, r4.x;
	IADD r19.x, r18.x, r5.x;
	MOV r20.x, i0.x;
	SHL r21.x, r20.x, 2;
	PADD ptr2, ptr1, r21.x;
	LD_RAW_PTR r22.x, ptr2;
	IADD r23.x, r19.x, r22.x;
	PADD ptr3, ptr0, r21.x;
	STORE_RAW_PTR ptr3.x, r23.x;
	SYNC.t.g;
	RET;

	END
